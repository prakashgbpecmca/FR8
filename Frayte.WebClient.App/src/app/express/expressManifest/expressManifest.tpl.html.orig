                                            <!-- ngIf: ::boundaryLinks -->
                                            <li class="pagination-first ng-scope disabled"><a href="" ng-click="selectPage(1, $event)" ng-disabled="noPrevious()||ngDisabled" uib-tabindex-toggle="" disabled="disabled" tabindex="-1">«</a></li><!-- end ngIf: ::boundaryLinks -->
                                            <!-- ngIf: ::directionLinks -->
                                            <li class="pagination-prev ng-scope disabled"><a href="" ng-click="selectPage(page - 1, $event)" ng-disabled="noPrevious()||ngDisabled" uib-tabindex-toggle="" disabled="disabled" tabindex="-1">‹</a></li><!-- end ngIf: ::directionLinks -->
                                            <!-- ngRepeat: page in pages track by $index -->
                                            <li class="pagination-page ng-scope active"><a href="" ng-click="selectPage(page.number, $event)" ng-disabled="ngDisabled&amp;&amp;!page.active" uib-tabindex-toggle="">1</a></li><!-- end ngRepeat: page in pages track by $index -->
                                            <li class="pagination-page ng-scope"><a href="" ng-click="selectPage(page.number, $event)" ng-disabled="ngDisabled&amp;&amp;!page.active" uib-tabindex-toggle="">2</a></li><!-- end ngRepeat: page in pages track by $index -->
                                            <!-- ngIf: ::directionLinks -->
                                            <li class="pagination-next ng-scope"><a href="" ng-click="selectPage(page + 1, $event)" ng-disabled="noNext()||ngDisabled" uib-tabindex-toggle="">›</a></li><!-- end ngIf: ::directionLinks -->
                                            <!-- ngIf: ::boundaryLinks -->
                                            <li class="pagination-last ng-scope"><a href="" ng-click="selectPage(totalPages, $event)" ng-disabled="noNext()||ngDisabled" uib-tabindex-toggle="">»</a></li><!-- end ngIf: ::boundaryLinks -->
