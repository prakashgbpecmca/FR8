

<section class="row ng-scope">
    <div class="row paddingboth" style="margin-right: 15px; margin-left: 15px;">
        <div id="metaltop-teal" style="padding: 0px 0px 0px; overflow: hidden;">
            <ul>
                <li class="first ng-scope">
                    <button class="btn btn-lg borderradius active" ng-class="{'active': activeRate(tab)}" ng-click="changeState(tab)">
                        {{'Menifest' | translate}}
                    </button>
                </li>
            </ul>
        </div>
    </div>
